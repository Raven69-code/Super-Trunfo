#include <stdio.h> // Inclui a biblioteca padrão de entrada e saída

int main() {
    // --- Dados do Rio de Janeiro ---
    char estado_rj[] = "Rio de Janeiro";
    char codigo_rj[] = "a01";
    long long populacao_rj = 6748000;      // População aproximada do Rio de Janeiro
    float tamanho_km2_rj = 1200.27;         // Área em km² do Rio de Janeiro
    double pib_rj = 380;                 // PIB em bilhões de reais do Rio de Janeiro
    int num_pontos_turisticos_rj = 58;      // Número de pontos turísticos do Rio de Janeiro

    printf("--- Informações sobre o Rio de Janeiro ---\n");
    printf("Código: %s\n", codigo_rj);
    printf("Estado: %s\n", estado_rj);
    printf("População: %lld habitantes\n", populacao_rj);
    printf("Tamanho: %.2f km²\n", tamanho_km2_rj);
    printf("PIB: R$ %.2f bilhões\n", pib_rj);
    printf("Número de pontos turísticos: %d\n", num_pontos_turisticos_rj);
    printf("\n"); // Adiciona uma linha em branco para separar as informações

    // --- Dados de São Paulo ---
    char estado_sp[] = "São Paulo";
    char codigo_sp[] = "a02";
    long long populacao_sp = 11452000;     // População aproximada de São Paulo (corrigido o formato)
    float tamanho_km2_sp = 1521.00;        // Área em km² de São Paulo (pode ser 1521.0 ou 1521)
    double pib_sp = 3500.00;                // PIB em bilhões de reais de São Paulo
    int num_pontos_turisticos_sp = 50;     // Número de pontos turísticos de São Paulo

    printf("--- Informações sobre São Paulo ---\n");
    printf("Código: %s\n", codigo_sp);
    printf("Estado: %s\n", estado_sp);
    printf("População: %lld habitantes\n", populacao_sp);
    printf("Tamanho: %.2f km²\n", tamanho_km2_sp);
    printf("PIB: R$ %.2f bilhões\n", pib_sp);
    printf("Número de pontos turísticos: %d\n", num_pontos_turisticos_sp);

    return 0; // Indica que o programa terminou com sucesso
}